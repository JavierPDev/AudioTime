{
  "name": "AudioTime",
  "version": "0.1.0",
  "description": "Timer/stopwatch with voice",
  "main": "index.js",
  "scripts": {
    "postinstall": "webdriver-manager update",
    "start": "webpack-dev-server --config webpack.dev.config.babel.js -d --inline",
    "build": "webpack --config webpack.prod.config.babel.js",
    "lint": "eslint .",
    "test": "npm run test.unit && npm run test.e2e.prod",
    "test.dev": "npm run test.unit && npm run test.e2e.dev",
    "test.unit": "karma start",
    "testd.unit": "karma start --auto-watch --no-single-run",
    "test.e2e.dev": "concurrently -k -s \"first\" \"npm start\" \"npm run protractor\"",
    "pretest.e2e.prod": "npm run build",
    "test.e2e.prod": "concurrently -k -s \"first\" \"npm run server.prod\" \"npm run protractor\"",
    "webdriver": "webdriver-manager start",
    "protractor": "protractor protractor.conf.js",
    "server.prod": "http-server dist || true"
  },
  "author": "Javier Pena <javier@javierdev.com>",
  "license": "MIT",
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.1",
    "eslint-loader": "^1.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "ng-annotate-loader": "^0.2.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.10.1",
    "protractor": "^4.0.9",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "webdriver-manager": "^10.2.4",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-foundation": "^0.8.0",
    "angular-ui-router": "^1.0.0-beta.3",
    "annyang": "^2.6.0",
    "foundation-sites": "5.5.2"
  }
}
